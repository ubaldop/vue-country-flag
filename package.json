{
  "name": "vue-country-flag",
  "version": "1.2.2",
  "description": "Simple Vue component for displaying of country flags",
  "main": "dist/country-flag.ssr.js",
  "module": "dist/country-flag.esm.js",
  "unpkg": "dist/country-flag.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/P3trur0/vue-country-flag.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue2",
    "flags",
    "country",
    "icon",
    "component"
  ],
  "author": "P3trur0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/P3trur0/vue-country-flag/issues"
  },
  "homepage": "https://github.com/P3trur0/vue-country-flag/",
  "dependencies": {},
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  }
}
