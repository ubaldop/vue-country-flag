{
  "name": "vue-country-flag",
  "version": "2.3.2",
  "description": "Simple Vue component for displaying of country flags",
  "main": "dist/country-flag.ssr.js",
  "module": "dist/country-flag.esm.js",
  "unpkg": "dist/country-flag.min.js",
  "browser": "dist/country-flag.esm.js",
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "tsc && cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "tsc && cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "tsc && cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "tsc && cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "test": "jest",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/P3trur0/vue-country-flag.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue2",
    "flags",
    "country",
    "icon",
    "component"
  ],
  "author": "P3trur0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/P3trur0/vue-country-flag/issues"
  },
  "homepage": "https://github.com/P3trur0/vue-country-flag/",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.5.1",
    "cross-env": "^7.0.3",
    "jest": "^25.5.4",
    "jest-transform-stub": "^2.0.0",
    "minimist": "^1.2.5",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^3.9.7",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=10"
  }
}
